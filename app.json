{
  "expo": {
    "name": "flexBox",
    "slug": "flexBox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/emparedado.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/emparedado.png",
      "resizeMode": "contain",
      "backgroundColor": "#49E615"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/emparedado.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.espinoxa.flexBox"
    },
    "web": {
      "favicon": "./assets/img/emparedado.png"
    }
  }
}
